class Solution {
    public boolean judgeCircle(String moves) {
        //[1,-1,1,-1]
        int x = 0,y = 0;
        // l => x += -1
        // r => x += 1
        // u => y += 1
        // d => y += -1
        for(char i : moves.toCharArray()){
            if(i == 'U') y += 1;
            else if(i == 'D') y += -1;
            else if(i == 'L') x += 1;
            else x += -1;
        }
        return (x == 0 && y == 0) ? true : false;
    }
}